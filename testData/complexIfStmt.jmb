int main(){
	//check that constants behave properly
	if 1 == 2 == 3 
		return 1
	if 1 == 1 == 1 
		print 1
	if true 
		print 2
	if false 
		return 1
	if 1 < 2 < 3 
		print 3
	if 1 <= 2 <= 2 
		print 4
	if 0-1 > 0-2 > 0-3 
		print 5
	if 0-1 >= 0-2 >= 0-2 
		print 6
		
	//Check that variables & inline-or-stmts work in if stmts
	int x = 0, y = 10
	if |x,y| == 0 print 7 
	if |x,y| == |5,10| print 8
	if 0 == |x| print 9
	if x != y print 10 

	//check that else-if stmts work 
	if x == 1 
		return 1 
	else if x+2 == 2 == y-8 
		print 11 
	else 
		return 1 
	
	//check that else stmts work
	if x == 1 
		return 1 
	else if y == 1 
		return 1 
	else 
		print 12
	
	return 0
}